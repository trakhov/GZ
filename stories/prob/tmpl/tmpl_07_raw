%{task} \begin{figure}[h!]\centering\small
\begin{tikzpicture}[x = 1cm,
		y = 1cm,
		line join = round,
		line cap = round,
		> = latex,		]
\draw[->]	(-1,0) -- (3,0)	node[right]	{$x$};
\draw[->]	(0,-.5) --  (0,1.5)	node[left]	{$f(x)$};
\draw[line width=1.5pt]		 (0,1) --  (2,0)
				(-1,0) -- (0,0)
				(2,0) -- (2.85,0);

\draw[dotted]		(2,0)  node[below] {$\vphantom{-}4$} -- (2,0);
\end{tikzpicture}
\end{figure}
